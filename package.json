{
  "name": "llm_council",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "npx tsx scripts/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:start": "docker run -d --name llm-council-db -e POSTGRES_USER=llm_council_user -e POSTGRES_PASSWORD=llm_council_password -e POSTGRES_DB=llm_council -p 5432:5432 postgres:15",
    "db:stop": "docker stop llm-council-db && docker rm llm-council-db",
    "redis:start": "docker run -d --name llm-council-redis -p 6379:6379 redis:7-alpine",
    "redis:stop": "docker stop llm-council-redis && docker rm llm-council-redis",
    "services:start": "docker-compose up -d",
    "services:stop": "docker-compose down",
    "services:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@types/validator": "^13.15.10",
    "canvas-confetti": "^1.9.4",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.26.2",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "next": "16.1.0",
    "openai": "^6.15.0",
    "pg": "^8.13.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "uuid": "^11.0.3",
    "validator": "^13.15.26",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
